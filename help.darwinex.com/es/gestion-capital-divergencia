<script>
  kbDom.whenReady(function() {
    document.querySelectorAll('.kb-category-menu svg').forEach(function(el) {
      el.addEventListener('click', function(e) {
        e.preventDefault();
        var li = kbDom.closest('li', this);
        if (li) {
          li.classList.toggle('open');
        }
      });
    })
  })
</script>
